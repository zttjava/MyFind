    <servlet-name>ArticleContentServletForA</servlet-name>
    <servlet-class>web.ArticleContentServletForA</servlet-class>
  </servlet>










  <servlet-mapping>
    <servlet-name>UserLoginServlet</servlet-name>
    <url-pattern>/servlet/UserLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserRegisterServlet</servlet-name>
    <url-pattern>/servlet/UserRegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserPostServlet</servlet-name>
    <url-pattern>/servlet/UserPostServlet</url-pattern>
  </servlet-mapping>
